<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:pages="clr-namespace:Lab1.Views"
                  x:Class="Lab1.Views.MasterView">


    <!--Menu-->
    <MasterDetailPage.Master>
        <pages:MenuView x:Name="MenuPage" x:Title="Titulo"/>  
    </MasterDetailPage.Master>


    <!--Pagina real-->
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:PersonaView/>
           </x:Arguments>        
        </NavigationPage>
    </MasterDetailPage.Detail>
    
</MasterDetailPage>
